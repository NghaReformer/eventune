{
  "name": "eventune-studios",
  "type": "module",
  "version": "1.0.0",
  "description": "Eventune Studios - Custom Songs for Special Moments",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "lint": "eslint src --ext .ts,.astro",
    "lint:fix": "eslint src --ext .ts,.astro --fix",
    "format": "prettier --write src",
    "typecheck": "astro check && tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "generate-types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID > src/types/database.types.ts"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/netlify": "^6.3.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "@aws-sdk/s3-request-presigner": "^3.700.0",
    "@supabase/supabase-js": "^2.49.9",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.8",
    "astro": "^5.16.6",
    "resend": "^4.2.0",
    "stripe": "^18.1.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.9.3",
    "zod": "^3.24.5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.15.8",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "eslint": "^9.23.0",
    "eslint-plugin-astro": "^1.3.1",
    "prettier": "^3.4.5",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "^0.6.12",
    "vitest": "^3.1.6"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
